class Node:
    def __init__(self,data):
        self.data=data
        self.leftchild= None 
        self.rightchild= None 
class BinarysearchTree:
    def __init__(self):
        self.root=None 
    def insert(self,data):
        if not self.root:
            self.root=Node(data)
        else: 
            self.insertNode(data,self.root)
    def insertNode(self,root,data):
